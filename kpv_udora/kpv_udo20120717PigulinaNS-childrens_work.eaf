    <TIER LINGUISTIC_TYPE_REF="ft-finT" PARENT_REF="ref@NSP-F-1956" PARTICIPANT="NSP-F-1956" TIER_ID="ft-fin@NSP-F-1956">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a23" ANNOTATION_REF="a1">
                <ANNOTATION_VALUE>Meille se oli taas työ. Meitä ei sinne pakotettu, äiti sanoi</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a24" ANNOTATION_REF="a2">
                <ANNOTATION_VALUE>olen niittänyt heinät siellä ja siellä, mene ja hae</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a25" ANNOTATION_REF="a3">
                <ANNOTATION_VALUE>Ja me kaikki tiesimme, että päivässä meidän pitää mennä neljästi tai viidesti hakemaan heinää </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a26" ANNOTATION_REF="a4">
                <ANNOTATION_VALUE>Sen jälkeen uit.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a27" ANNOTATION_REF="a5">
                <ANNOTATION_VALUE></ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a28" ANNOTATION_REF="a6">
                <ANNOTATION_VALUE>Ja jos et tehnyt sitä niin hävetti, miten muka, ei ei, edes puhua moisesta ei voinut.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a29" ANNOTATION_REF="a7">
                <ANNOTATION_VALUE>Että äitisi ja isäsi työskentelevät ja itse vain uit ja kuljeskelet.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a30" ANNOTATION_REF="a8">
                <ANNOTATION_VALUE>Kukaan ei tehnyt täällä niin.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a31" ANNOTATION_REF="a9">
                <ANNOTATION_VALUE>Oli heitä, mutta sellaisia oli hyvin vähän, jotka eivät tehneet mitään.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a32" ANNOTATION_REF="a10">
                <ANNOTATION_VALUE>Työskentelivät niin lapset kuin aikuisetkin, ainoastaan työtä, työtä, työtä.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
    </TIER>
